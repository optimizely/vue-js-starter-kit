{
  "name": "VueJS Starter Kit",

  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },

  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "Vue.volar",
        "Vue.vscode-typescript-vue-plugin",
        "esbenp.prettier-vscode"
      ]
    }
  },

  "postCreateCommand": "npm install",
  "remoteUser": "node"
}
